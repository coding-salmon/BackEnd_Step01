  <display-name>_08_MemberListServlet</display-name>
  
    <!-- 서블릿 선언(톰캣이 web.xml을 읽어서, 객체 생성 후 브라우저에서 매핑된 주소가 들어오면 이 객체를 호출한다. 
  <servlet>
  	<servlet-name>list</servlet-name>
  	<servlet-class>spms.sevlets.MemberListServlet</servlet-class>
  </servlet>-->
  
  <!-- 주소와 매핑 
  <servlet-mapping>
